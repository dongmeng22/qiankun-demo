    "ins:vue3": "cd vue3 && pnpm i",
    "ins:react": "cd qiankun-react && pnpm i",
    "start": "npm-run-all --parallel start:*",
    "start:base": "cd base && pnpm run dev",
    "start:vue3": "cd vue3 && pnpm run dev",
    "start:react": "cd qiankun-react && pnpm run dev",
    "build": "npm-run-all --parallel build:*",
    "build:base": "cd base && pnpm run build",
    "build:vue3": "cd vue3 && pnpm run build",
    "build:react": "cd qiankun-react && pnpm run build"
